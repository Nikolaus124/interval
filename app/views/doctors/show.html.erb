<!doctype html>
<html lang="en">
<head>

  <% @title = "Doctors" %>

</head>
<body>

  <table>
    <thead>
    <tr>
      <th>Position</th>
      <th>Name</th>
      <th>Worktime</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @doctors.each do |d| %>
      <tr>
        <td><%= d.position %></td>
        <td><%= d.first_name %> <%= d.last_name %></td>
        <td><%= d.begin %>-<%= d.end %></td>
      </tr>
    <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to "Back", root_path %>

</body>
</html>